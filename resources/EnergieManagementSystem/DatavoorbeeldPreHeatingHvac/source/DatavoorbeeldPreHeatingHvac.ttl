PREFIX org:                        <http://www.w3.org/ns/org#>
PREFIX productieverlies:           <https://example.com/concept/productieimpacttype/productieverlies/>
PREFIX qudt:                       <https://qudt.org/schema/qudt/>
PREFIX rdf:                        <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:                       <http://www.w3.org/2000/01/rdf-schema#>
PREFIX saref:                      <https://saref.etsi.org/core/>
PREFIX schema:                     <https://schema.org/>
PREFIX seb:                        <https://data.vlaanderen.be/ns/sensoren-en-bemonstering#>
PREFIX skos:                       <http://www.w3.org/2004/02/skos/core#>
PREFIX sosa:                       <http://www.w3.org/ns/sosa/>
PREFIX sosas:                      <https://www.w3.org/ns/sosa/>

PREFIX utility-services:           <https://vocab.belgif.be/ns/utility-services#>
PREFIX warmteverliesklasse:        <https://example.com/concept/warmteverliesklasse/>
PREFIX xs:                         <http://www.w3.org/2001/XMLSchema#>

unit:PERCENT  rdf:type  qudt:Unit .

unit:DEG_C  rdf:type  qudt:Unit .

[ rdf:type                   sosa:Observation;
  dct:type                   observatietype:temporele_observatie;
  sosa:hasFeatureOfInterest  _:b0;
  sosa:hasResult             [ rdf:type            time:Instant;
                               time:inXSDDateTime  "20250401T05:30:00.000"^^xs:dateTime
                             ];
  sosa:madeBySensor          [ rdf:type       it6:MachineLearningModel , sosas:Sensor;
                               it6:trainedOn  [ rdf:type              ssn:Input;
                                                dct:type              inputtype:observatieverzameling;
                                                seb:Input.referentie  "_:OV001"^^xs:anyURI
                                              ];
                               dct:type       "https://example.com/concept/sensortype/random_forest_regressiemodel"
                             ];
  sosa:observedProperty      observeerbaar_kenmerk:tijdstip_start_HVAC;
  sosa:usedProcedure         [ rdf:type       <http://www.w3.org/ns/sosa/oms/ObservingProcedure>;
                               dct:type       observatieproceduretype:voorspelling_op_basis_van_observaties;
                               sosa:hasInput  [ rdf:type              ssn:Input;
                                                dct:type              inputtype:doeltijdstip;
                                                seb:Input.referentie  [ rdf:type            time:Instant;
                                                                        time:inXSDDateTime  "20250401T08:00:00.000"^^xs:dateTime
                                                                      ]
                                              ];
                               sosa:hasInput  [ rdf:type              ssn:Input;
                                                dct:type              inputtype:doeltemperatuur;
                                                seb:Input.referentie  [ rdf:type         schema:QuantitativeValue;
                                                                        schema:unitCode  unit:DEG_C;
                                                                        schema:value     "20"^^rdfs:Literal
                                                                      ]
                                              ];
                               sosa:hasInput  [ rdf:type              ssn:Input;
                                                dct:type              inputtype:observatie;
                                                seb:Input.referentie  "_:OB005"^^xs:anyURI
                                              ];
                               sosa:hasInput  [] ;
                               sosa:hasInput  [] ;
                               sosa:hasInput  [ rdf:type              ssn:Input;
                                                dct:type              inputtype:observatie;
                                                seb:Input.referentie  "_:OB002"^^xs:anyURI
                                              ]
                             ]
] .

[ rdf:type             sosa:ObservationCollection;
  sosa:phenomenonTime  [ rdf:type           time:ProperInterval;
                         time:hasBeginning  [ rdf:type            time:Instant;
                                              time:inXSDDateTime  "20250101T00:00:00.000"^^xs:dateTime
                                            ];
                         time:hasEnd        [ rdf:type            time:Instant;
                                              time:inXSDDateTime  "20250331T00:00:00.000"^^xs:dateTime
                                            ]
                       ]
] .

[ rdf:type                   sosa:Observation;
  dct:type                   observatietype:meting;
  sosa:hasFeatureOfInterest  _:b0;
  sosa:hasResult             [ rdf:type         schema:QuantitativeValue;
                               schema:unitCode  unit:PERCENT;
                               schema:value     "80"^^rdfs:Literal
                             ];
  sosa:observedProperty      observeerbaar_kenmerk:verwachte_betzetting;
  sosa:phenomenonTime        [ rdf:type            time:Instant;
                               time:inXSDDateTime  "20250401T08:00:00.000"^^xs:dateTime
                             ];
  sosa:usedProcedure         [ rdf:type  <http://www.w3.org/ns/sosa/oms/ObservingProcedure>;
                               dct:type  observatieproceduretype:voorspelling_volgens_badgelogs
                             ]
] .

[ rdf:type                   sosa:Observation;
  dct:type                   observatietype:meting;
  sosa:hasFeatureOfInterest  _:b0;
  sosa:hasResult             [ rdf:type         schema:QuantitativeValue;
                               schema:unitCode  unit:DEG_C;
                               schema:value     "1"^^rdfs:Literal
                             ];
  sosa:observedProperty      observeerbaar_kenmerk:buitentemperatuur;
  sosa:phenomenonTime        [ rdf:type            time:Instant;
                               time:inXSDDateTime  "20250401T06:00:00.000"^^xs:dateTime
                             ];
  sosa:usedProcedure         [ rdf:type  <http://www.w3.org/ns/sosa/oms/ObservingProcedure>;
                               dct:type  observatieproceduretype:online_opvraging
                             ]
] .

_:b0    rdf:type                   utility-services:Connection , sosa:Actuation;
        sosa:actsOnProperty        observeerbaar_kenmerk:toestelstatus;
        sosa:hasFeatureOfInterest  [ rdf:type  saref:Device;
                                     dct:type  energiebrontype:warmtepomp_elektriciteit , toesteltype:HVAC_verwarming
                                   ];
        sosa:hasResult             toestelstatus:aan;
        sosa:resultTime            [ rdf:type            time:Instant;
                                     time:inXSDDateTime  "20250401T05:30:00.000"^^xs:dateTime
                                   ];
        sosa:usedProcedure         [ rdf:type       sosa:Procedure;
                                     dct:type       aansturingsproceduretype:aansturing_op_basis_van_waarnemingen;
                                     sosa:hasInput  [ rdf:type              ssn:Input;
                                                      dct:type              inputtype:observatie;
                                                      seb:Input.referentie  "_:OB001"^^xs:anyURI
                                                    ]
                                   ];
        utility-services:address   [ rdf:type          locn:Address;
                                     locn:fullAddress  "Simon Bolivarlaan 17, 1030 Schaarbeek"@nl;
                                     locn:locatorName  "Vlaamse Overheid - Belpaire Gebouw"@nl
                                   ] .

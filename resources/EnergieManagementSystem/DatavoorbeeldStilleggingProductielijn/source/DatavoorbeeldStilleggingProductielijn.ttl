unit:HR  rdf:type  qudt:Unit .

unit:EUR-PER-MegaW-HR
        rdf:type  qudt:Unit .

[ rdf:type                   sosa:Observation;
  dct:type                   observatietype:meting;
  sosa:hasFeatureOfInterest  [ rdf:type                   utility-services:Connection , sosa:Actuation;
                               sosa:actsOnProperty        aanstuurbaar_kenmerk:toestelstatus;
                               sosa:hasFeatureOfInterest  [ rdf:type  saref:Device;
                                                            dct:type  domeinobjecttype:productielijn_houtbewerking
                                                          ];
                               sosa:hasResult             toestelstatus:uit;
                               sosa:madeByActuator        [ rdf:type  sosa:Actuator;
                                                            dct:type  aansturingssysteemtype:controller
                                                          ];
                               sosa:resultTime            [ rdf:type           time:ProperInterval;
                                                            time:hasBeginning  [ rdf:type            time:Instant;
                                                                                 time:inXSDDateTime  "20250214T14:00:00.000"^^xs:dateTime
                                                                               ];
                                                            time:hasEnd        [ rdf:type            time:Instant;
                                                                                 time:inXSDDateTime  "20250214T18:00:00.000"^^xs:dateTime
                                                                               ]
                                                          ];
                               sosa:usedProcedure         [ rdf:type       sosa:Procedure;
                                                            dct:type       aansturingsproceduretype:statusaanpassing_volgens_elektriciteitsprijs;
                                                            sosa:hasInput  [ rdf:type              ssn:Input;
                                                                             dct:type              inputtype:observatie;
                                                                             seb:Input.referentie  "_:OB001"^^xs:anyURI
                                                                           ];
                                                            sosa:hasInput  [ rdf:type              ssn:Input;
                                                                             dct:type              inputtype:maximale_duur_stillegging;
                                                                             seb:Input.referentie  [ rdf:type         schema:QuantitativeValue;
                                                                                                     schema:unitCode  unit:HR;
                                                                                                     schema:value     "8"^^rdfs:Literal
                                                                                                   ]
                                                                           ];
                                                            sosa:hasInput  [ rdf:type              ssn:Input;
                                                                             dct:type              inputtype:minimale_duur_stillegging;
                                                                             seb:Input.referentie  [ rdf:type         schema:QuantitativeValue;
                                                                                                     schema:unitCode  unit:HR;
                                                                                                     schema:value     "4"^^rdfs:Literal
                                                                                                   ]
                                                                           ];
                                                            sosa:hasInput  [ rdf:type              ssn:Input;
                                                                             dct:type              inputtype:elektriciteitsprijs_drempelwaarde;
                                                                             seb:Input.referentie  [ rdf:type         schema:QuantitativeValue;
                                                                                                     schema:unitCode  unit:EUR-PER-MegaW-HR;
                                                                                                     schema:value     "100"^^rdfs:Literal
                                                                                                   ]
                                                                           ]
                                                          ];
                               utility-services:address   [ rdf:type          locn:Address;
                                                            locn:fullAddress  "Voorbeeldstraat 123, 8500 Kortrijk"@nl
                                                          ]
                             ];
  sosa:hasResult             [ rdf:type         schema:QuantitativeValue;
                               schema:unitCode  unit:EUR-PER-MegaW-HR;
                               schema:value     "105"^^rdfs:Literal
                             ];
  sosa:observedProperty      observeerbaar_kenmerk:day_ahead_elektriciteitsprijs;
  sosa:phenomenonTime        [ rdf:type           time:ProperInterval;
                               time:hasBeginning  [ rdf:type            time:Instant;
                                                    time:inXSDDateTime  "20250214T14:00:00.000"^^xs:dateTime
                                                  ];
                               time:hasEnd        [ rdf:type            time:Instant;
                                                    time:inXSDDateTime  "20250214T20:00:00.000"^^xs:dateTime
                                                  ]
                             ];
  sosa:usedProcedure         [ rdf:type  <http://www.w3.org/ns/sosa/oms/ObservingProcedure>;
                               dct:type  observatieproceduretype:online_opvraging
                             ]
] .

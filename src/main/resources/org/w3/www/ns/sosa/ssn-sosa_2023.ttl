ssn:    rdf:type             owl:Ontology;
        rdfs:comment         "Please report any errors to the W3C Spatial Data on the Web Working Group via the SDW WG Public List public-sdw-wg@w3.org"@en;
        rdfs:seeAlso         <https://www.w3.org/2015/spatial/wiki/Semantic_Sensor_Network_Ontology>;
        dcterms:created      "2017-04-17"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];
        dcterms:description  "This ontology describes sensors, actuators and observations, and related concepts. It does not describe domain concepts, time, locations, etc. these are intended to be included from other ontologies via OWL imports."@en , "\n  This module imports all the axiomatizations of the SOSA terms, and adds cross-module axioms. \n  "@en;
        dcterms:license      <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document> , <http://www.opengeospatial.org/ogc/Software>;
        dcterms:modified     "2024-02-03"^^xsd:date;
        dcterms:rights       "Copyright 2025 W3C/OGC.";
        dcterms:title        "Semantic Sensor Network Ontology"@en;
        owl:imports          sosa: , ssn-dep: , ssn-sam: , ssn-obs: , ssn-act:;
        owl:priorVersion     <http://www.w3.org/ns/ssn/2017/>;
        owl:versionIRI       <http://www.w3.org/ns/ssn/2023/>;
        owl:versionInfo      "New modular version of the SSN ontology. \n\nThis ontology was originally developed in 2009-2011 by the W3C Semantic Sensor Networks Incubator Group (SSN-XG). For more information on the group's activities see: http://www.w3.org/2005/Incubator/ssn/. The ontology was revised and modularized in 2015-2017 by the W3C/OGC Spatial Data on the Web Working Group, see: https://www.w3.org/2015/spatial/wiki/Semantic_Sensor_Network_Ontology. \n\nIn particular, (a) the scope is extended to include actuation and sampling; (b) the core concepts and properties are factored out into the SOSA ontology. The SSN ontology imports SOSA and adds formal axiomatization consistent with the text definitions in SOSA, and adds classes and properties to accommodate the scope of the original SSN ontology. "@en;
        skos:historyNote     "\n  Terms in the ssn: namespace have equivalence axioms to terms declared in the sosa: namespace, and are marked 'deprecated'. \n  The axiomatization from the 2017 version of the standard is preserved. \n\n  Terms in the sosa: namespace do not carry symmetric equivalent axioms. \n  Axiomatization only uses sosa: terms. \n  ";
        skos:scopeNote       "The SSN module contains an axiomatization of the core terms from the SOSA module (serialized in the sosa:ttl file) plus some deprecated terms in the ssn: namespace. " .

sosa-obs:  rdf:type          owl:Ontology;
        dcterms:created      "2024-02-02"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];
        dcterms:description  "Observation classes and properties for the SSN Ontology."@en;
        dcterms:license      <http://www.opengeospatial.org/ogc/Software> , <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document>;
        dcterms:rights       "Copyright 2025 W3C/OGC.";
        dcterms:title        "Sensor, Observation, Sample, and Actuator (SOSA) Ontology - actuation module"@en;
        owl:imports          sosa-common: .

sosa:wasActedOnBy  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "was acted on by"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:ActuationCollection , sosa:Actuation;
        owl:inverseOf          sosa:actsOnProperty;
        skos:definition        "\n  relation from a Property to an Actuation that changed its state\n  "@en;
        skos:example           "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property upon which the Actuator acts is the state of the window as it changes from being open to being closed. "@en .

sosa:madeByActuator  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "made by actuator"@en;
        rdfs:subPropertyOf     sosa:madeBySystem;
        schema:domainIncludes  sosa:ActuationCollection , sosa:Actuation;
        schema:rangeIncludes   sosa:Actuator;
        owl:inverseOf          sosa:madeActuation;
        skos:definition        "\n  relation from an Actuation or ActuationCollection to the Actuator that made it\n  "@en .

sosa:madeActuation  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "made actuation"@en;
        rdfs:subPropertyOf     sosa:madeExecution;
        schema:domainIncludes  sosa:Actuator;
        schema:rangeIncludes   sosa:ActuationCollection , sosa:Actuation;
        owl:inverseOf          sosa:madeByActuator;
        skos:definition        "\n  relation from an Actuator to an Actuation or ActuationCollection it has made\n  "@en .

sosa:isActedOnBy  rdf:type     owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "is acted on by"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:Actuator;
        owl:inverseOf          sosa:actsOn;
        skos:definition        "\n  relation from a Property to an Actuator that may change its state\n  "@en;
        skos:example           "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property upon which the Actuator acts is the state of the window as it changes from being open to being closed. "@en .

sosa:actsOnProperty  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "acts on property"@en;
        schema:domainIncludes  sosa:ActuationCollection , sosa:Actuation;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:wasActedOnBy;
        skos:definition        "\n  relation from an Actuation to the Property it acted upon\n  "@en;
        skos:example           "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property upon which the Actuator acts is the state of the window as it changes from being open to being closed. "@en .

sosa:actsOn  rdf:type          owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-act:;
        rdfs:label             "acts on"@en;
        rdfs:subPropertyOf     sosa:forProperty;
        schema:domainIncludes  sosa:Actuator;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:isActedOnBy;
        skos:definition        "\n  relation from an Actuator to a Property whose state may be changed by using it\n  "@en;
        skos:example           "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property upon which the Actuator acts is the state of the window as it changes from being open to being closed. "@en .

sosa:Actuator  rdf:type   owl:Class;
        rdfs:isDefinedBy  sosa-act:;
        rdfs:label        "Actuator"@en;
        rdfs:subClassOf   sosa:System;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Actuation sosa:ActuationCollection )
                                               ];
                            owl:onProperty     sosa:madeActuation
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ActuatingProcedure;
                            owl:onProperty     sosa:implements
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:actsOn
                          ];
        skos:definition   "\n  System that implements an ActuatingProcedure to change the value of an actuatable Property\n\n  This may be a device, a piece of infrastructure, an agent (including humans), or software (simulation)."@en;
        skos:example      "A window actuator for automatic window control, i.e., opening or closing the window."@en .

sosa:ActuationCollection
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-act:;
        rdfs:label        "Actuation Collection"@en;
        rdfs:subClassOf   sosa:ExecutionCollection;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Actuation sosa:ActuationCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ActuatingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:actsOnProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Actuator;
                            owl:onProperty     sosa:madeByActuator
                          ];
        owl:disjointWith  sosa:SamplingCollection , sosa:SampleCollection , sosa:ObservationCollection;
        skos:definition   "\n  set of one or more Actuations or ActuationCollections; collections may be nested\n\n  All Actuation properties may appear in an Actuation Collection, including:\n  hasFeatureOfInterest , usedProcedure , madeByActuator , actsOnProperty , endTime ,  hasResult , hasSimpleResult . \n  If they are present, the value of these properties summarize the values of the matching properties of the member actuations, where membership is either direct or transitive through one or more member actuation collections. \n  The following consistency rules apply with respect to the Actuation properties: \n  \n  1. Where an individual Actuation Collection omits a property, a member Actuation (direct or transitive) MAY have any value for that property.\n\n  2. Where an individual Actuation Collection has a single value for a property, each member Actuation (direct or transitive) MUST have that same value for the property - i.e., the collection is homogeneous in that property.  That property MAY then be omitted in any member Actuation or Actuation Collection.\n\n  3. Where an individual Actuation Collection has more than one value for a property, each member Actuation (direct or transitive) MUST have a value for that property that matches one of the values for the property in the collection. \n  \n  4. Where an individual Actuation Collection has a value for a property that is a range or interval, each member Actuation (direct or transitive) MUST have a value for that property that matches or falls within that range or interval.\n\n  5. Where an individual Actuation Collection has more than one value for a property that is a range or interval, each member Actuation (direct or transitive) MUST have a value for that property that either matches or falls within one of those ranges or intervals.\n  "@en;
        skos:note         "The members of a collection do not necessarily share a common value for any property. "@en .

sosa:Actuation  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-act:;
        rdfs:label        "Actuation"@en;
        rdfs:subClassOf   sosa:Execution;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ActuatingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:actsOnProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Actuator;
                            owl:onProperty     sosa:madeByActuator
                          ];
        skos:definition   "\n  act of carrying out an ActuatingProcedure using an Actuator to change the value of an actuatable Property of a FeatureOfInterest\n  \n  In this context hasResult may indicate the value of the Property at the completion of the Actuation\n  "@en;
        skos:example      "The activity of automatically closing a window if the temperature in a room drops below 20 degree Celsius. The activity is the Actuation and the device that closes the window is the Actuator. The Procedure is the rule, plan, or specification that defines the conditions that triggers the Actuation, here a drop in temperature. "@en;
        skos:note         "relatedObservation SHOULD be used to link to an associated Observation or ObservationCollection that monitors the actuated property to verify that the result value has been obtained. "@en;
        skos:scopeNote    "\n  An Actuation primarily concerns a single Property of a single FeatureOfInterest. \n  Changes to multiple properties, or other combinations of Actuations, may be described in an ActuationCollection.\n    \n  Alternatively, applications may choose to encapsulate complexity by defining a complex property with multiple individual components, and a corresponding complex result (e.g., a vector). \n  This approach is not prohibited by the SSN Ontology, but the details are beyond the scope of SSN.\n  "@en .

sosa:ActuatingProcedure
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-act:;
        rdfs:label        "Actuating Procedure"@en;
        rdfs:subClassOf   sosa:Procedure;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Actuator;
                            owl:onProperty     sosa:implementedBy
                          ];
        skos:definition   "\n  workflow, protocol, plan, algorithm, or computational method specifying how to make an Actuation\n  \n  An Actuating Procedure is re-usable, and might be applied in many Actuations.\n  It explains the steps to be carried out to arrive at reproducible results.\n  "@en .

ssn-act:  rdf:type           owl:Ontology;
        dcterms:created      "2024-02-02"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];
        dcterms:description  "Actuation classes and properties for the SSN Ontology.\n  This module adds RDFS and OWL axioms to the terms defined in SOSA Actuation\n  "@en , "Actuation classes and properties for the SSN Ontology."@en;
        dcterms:license      <http://www.opengeospatial.org/ogc/Software> , <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document>;
        dcterms:rights       "Copyright 2025 W3C/OGC.";
        dcterms:title        "SSN Ontology - actuation module"@en , "Sensor, Observation, Sample, and Actuator (SOSA) Ontology - actuation module"@en;
        owl:imports          ssn-common: , sosa-act: .

sosa:usedForExecutionHasResult
        rdf:type                owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-sam:;
        rdfs:label              "used for execution has result"@en;
        schema:domainIncludes   sosa:SamplingProcedure;
        schema:rangeIncludes    sosa:SampleCollection , sosa:Sample;
        owl:inverseOf           sosa:isResultOfUsedProcedure;
        owl:propertyChainAxiom  ( sosa:usedForExecution sosa:hasResult );
        skos:definition         "\n  relation from a Procedure to a Sample or SampleCollection made by it\n  "@en .

sosa:madeSamplingHasResult
        rdf:type                owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-sam:;
        rdfs:label              "made sampling has result"@en;
        schema:domainIncludes   sosa:Sampler;
        schema:rangeIncludes    sosa:SampleCollection , sosa:Sample;
        owl:inverseOf           sosa:isResultOfMadeBySampler;
        owl:propertyChainAxiom  ( sosa:madeSampling sosa:hasResult );
        skos:definition         "\n  relation from a Sampler to a Sample or SampleCollection made by it\n  "@en .

sosa:madeSampling  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "made sampling"@en;
        rdfs:subPropertyOf     sosa:madeExecution;
        schema:domainIncludes  sosa:Sampler;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling;
        owl:inverseOf          sosa:madeBySampler;
        skos:definition        "\n  relation from a Sampler to the Sampling or SamplingCollection it has made\n  "@en .

sosa:madeBySampler  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "made by sampler"@en;
        rdfs:subPropertyOf     sosa:madeBySystem;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling;
        schema:rangeIncludes   sosa:Sampler;
        owl:inverseOf          sosa:madeSampling;
        skos:definition        "\n  relation from an act of Sampling to the Sampler that made it\n  "@en .

sosa:isSampleOf  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "is sample of"@en;
        rdfs:subPropertyOf     prov:wasDerivedFrom;
        schema:domainIncludes  sosa:SampleCollection , sosa:Sample;
        schema:rangeIncludes   sosa:FeatureOfInterest;
        owl:inverseOf          sosa:hasSample;
        skos:definition        "\n  relation from a Sample or SampleCollection to the FeatureOfInterest that it is intended to represent\n  "@en .

sosa:isResultOfUsedProcedure
        rdf:type                owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-sam:;
        rdfs:label              "is a result of used procedure"@en;
        schema:domainIncludes   sosa:SampleCollection , sosa:Sample;
        schema:rangeIncludes    sosa:SamplingProcedure;
        owl:inverseOf           sosa:usedForExecutionHasResult;
        owl:propertyChainAxiom  ( sosa:isResultOf sosa:usedProcedure );
        skos:definition         "\n  relation from a Sample or SampleCollection to the Procedure (plan) used to make it\n  "@en .

sosa:isResultOfMadeBySampler
        rdf:type                owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-sam:;
        rdfs:label              "is a result of made by sampler"@en;
        schema:domainIncludes   sosa:SampleCollection , sosa:Sample;
        schema:rangeIncludes    sosa:Sampler;
        owl:inverseOf           sosa:madeSamplingHasResult;
        owl:propertyChainAxiom  ( sosa:isResultOf sosa:madeBySampler );
        skos:definition         "\n  relation from a Sample or SampleCollection to the Sampler that made it\n  "@en .

sosa:isOriginalSampleOf
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "is original sample  of"@en;
        schema:domainIncludes  sosa:Sample;
        schema:rangeIncludes   sosa:SampleCollection , sosa:Sample;
        owl:inverseOf          sosa:hasOriginalSample;
        skos:definition        "\n  relation from the initial Sample collected from the ultimate FeatureOfInterest to a downstream Sample or SampleCollection\n  "@en .

sosa:isSampleOfUltimateFOI
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "has sampled feature"@en;
        schema:domainIncludes  sosa:SampleCollection , sosa:Sample;
        schema:rangeIncludes   sosa:FeatureOfInterest;
        owl:inverseOf          sosa:featureHasUltimateSample;
        skos:definition        "\n  relation from a Sample or SampleCollection to the ultimate FeatureOfInterest that it is intended to be representative of; i.e., the end of a chain of isSampleOf relations\n  "@en .

sosa:hasSample  rdf:type       owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "has sample"@en;
        schema:domainIncludes  sosa:FeatureOfInterest;
        schema:rangeIncludes   sosa:Sample;
        owl:inverseOf          sosa:isSampleOf;
        skos:definition        "\n  relation from a FeatureOfInterest to the Sample or SampleCollection used to represent it\n  "@en .

sosa:hasOriginalSample
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "has original sample"@en;
        schema:domainIncludes  sosa:SampleCollection , sosa:Sample;
        schema:rangeIncludes   sosa:Sample;
        owl:inverseOf          sosa:isOriginalSampleOf;
        skos:definition        "\n  relation from a Sample or SampleCollection to the initial Sample collected from the ultimate FeatureOfInterest\n  "@en .

sosa:featureHasUltimateSample
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-sam:;
        rdfs:label             "feature has ultimate sample"@en;
        schema:domainIncludes  sosa:FeatureOfInterest;
        schema:rangeIncludes   sosa:SampleCollection , sosa:Sample;
        owl:inverseOf          sosa:isSampleOfUltimateFOI;
        skos:definition        "\n  relation from an ultimate FeatureOfInterest to a Sample or SampleCollection that is intended to be representative of it; i.e., the end of a chain of hasSample relations\n  "@en .

sosa:StatisticalSample
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Statistical Sample"@en;
        rdfs:subClassOf   sosa:Sample;
        skos:definition   "\n  subset of a FeatureOfInterest defined by the statistical process used to create it\n  "@en;
        skos:example      "The male or female subset of a population."@en;
        skos:note         "StatisticalSamples usually apply to populations or other sets, of which a certain subset may be of specific interest."@en .

sosa:SpatialSample  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Spatial Sample"@en;
        rdfs:subClassOf   sosa:Sample;
        skos:definition   "\n  subset of a FeatureOfInterest defined by its location and shape in space\n  \n  When observations are made to estimate properties of a geospatial feature, particularly where the value of a property varies within the scope of the feature, a SpatialSample is used.\n  "@en;
        skos:example      "Some common names for spatial samples include: borehole, flightline, interval, lidar cloud, map horizon, microscope slide, mine level, mine, observation well, profile,  quadrat, scene, section, ship track, spot, station, swath, trajectory, traverse, etc..."@en;
        skos:note         "Depending on accessibility and on the nature of the expected property variation, a SpatialSample may extend in one, two or three spatial dimensions."@en .

sosa:SamplingProcedure
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sampling Procedure"@en;
        rdfs:subClassOf   sosa:Procedure;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sampler;
                            owl:onProperty     sosa:implementedBy
                          ];
        skos:definition   "\n  workflow, protocol, plan, algorithm, or computational method specifying how to make a Sampling\n\n  A Sampling Procedure is re-usable, and might be involved in many Samplings. \n  It explains the steps to be carried out to arrive at reproducible results.\n  "@en;
        skos:example      "\nspatial distribution of locations where to collect a sample (e.g., river segment)\ninstrument (or mode of use thereof) required to collect a sample (e.g., fish net, electric stunner)\nfield work instructions (e.g., dig four boreholes in each cardinal direction 2 metre way from the centre of the site; collect soil material from the 15-30 cm depth and mix it in a single bag)\n  "@en;
        skos:note         "\n[ISO 19156:2023] A SamplingProcedure shall be defined as the description of steps performed by a Sampler in order to extract a Sample from its sampledFeature in the frame of a Sampling.\n  "@en .

sosa:SamplingCollection
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sampling Collection"@en;
        rdfs:subClassOf   sosa:ExecutionCollection;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Sampling sosa:SamplingCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Sampling sosa:SamplingCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sample;
                            owl:onProperty     sosa:hasResult
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:SamplingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sampler;
                            owl:onProperty     sosa:madeBySampler
                          ];
        owl:disjointWith  sosa:SampleCollection , sosa:ObservationCollection , sosa:ActuationCollection;
        skos:definition   "\n  set of one or more Samplings or SamplingCollections; collections may be nested\n\n  All Sampling properties may appear in a Sampling Collection, including: \n  hasFeatureOfInterest , hasUltimateFeatureOfInterest , madeBySampler , usedProcedure , hasInputValue , startTime , phenomenonTime , resultTime , hasResult . \n  If they are present, the value of these properties summarize the values of the matching properties of the member samplings, where membership is either direct or transitive through one or more member sampling collections. \n  The following consistency rules apply with respect to the Sampling properties listed above: \n  \n  1. Where an individual Sampling Collection omits a property, a member Sampling (direct or transitive) MAY have any value for that property.\n\n  2. Where an individual Sampling Collection has a single value for a property, each member Sampling (direct or transitive) MUST have that same value for the property - i.e., the collection is homogeneous in that property.  That property MAY then be omitted in any member Sampling or Sampling Collection.\n\n  3. Where an individual Sampling Collection has more than one value for a property, each member Sampling (direct or transitive) MUST have a value for that property that matches one of the values for the property in the collection. \n  \n  4. Where an individual Sampling Collection has a value for a property that is a range or interval, each member Sampling (direct or transitive) MUST have a value for that property that matches or falls within that range or interval.\n\n  5. Where an individual Sampling Collection has more than one value for a property that is a range or interval, each member Sampling (direct or transitive) MUST have a value for that property that either matches or falls within one of those ranges or intervals.\n  "@en;
        skos:note         "The members of a collection do not necessarily share a common value for any property."@en .

sosa:Sampling  rdf:type   owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sampling"@en;
        rdfs:subClassOf   sosa:Execution;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sampler;
                            owl:onProperty     sosa:madeBySampler
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:SamplingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sample;
                            owl:onProperty     sosa:hasResult
                          ];
        skos:definition   "\n  act of carrying out a SamplingProcedure using a Sampler to create one or more Samples of a FeatureOfInterest\n  "@en;
        skos:example      "Taking a diamond-drill core from a rock outcrop."@en , "Splitting a piece of drill-core to create two new samples."@en , "Selecting a subset of a population."@en , "Sieving a powder to separate the subset finer than 100-mesh."@en , "Crushing a rock sample in a ball mill."@en , "Digging a pit through a soil sequence."@en , "Dividing a field site into quadrants."@en , "Drawing blood from a patient."@en , "Drilling an observation well."@en , "Establishing a station for environmental monitoring."@en , "Registering an image of the landscape."@en .

sosa:Sampler  rdf:type    owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sampler"@en;
        rdfs:subClassOf   sosa:System;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Sampling sosa:SamplingCollection )
                                               ];
                            owl:onProperty     sosa:madeSampling
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:SamplingProcedure;
                            owl:onProperty     sosa:implements
                          ];
        skos:definition   "\n  System that implements a SamplingProcedure to generate a Sample\n  "@en;
        skos:example      "A ball mill, diamond drill, hammer, hypodermic syringe and needle, image Sensor, or a soil auger can all act as Samplers. Sometimes, however, the distinction between the Sampler and the Sensor is not evident, if they are packaged as a unit."@en;
        skos:note         "A Sampler need not be a physical device."@en .

sosa:SampleCollection
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sample Collection"@en;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Sample sosa:SampleCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Sampling sosa:SamplingCollection )
                                               ];
                            owl:onProperty     sosa:isResultOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:isSampleOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sample;
                            owl:onProperty     sosa:hasOriginalSample
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:isSampleOfUltimateFOI
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sampler;
                            owl:onProperty     sosa:isResultOfMadeBySampler
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:SamplingProcedure;
                            owl:onProperty     sosa:isResultOfUsedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Sample sosa:SampleCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        owl:disjointWith  sosa:ActuationCollection , sosa:ObservationCollection , sosa:SamplingCollection;
        skos:definition   "\n  set of one or more Samples or SampleCollections; collections may be nested.\n\n  All Sample properties may appear in a Sample Collection, including: \n  hasOriginalSample , isSampleOfUltimateFOI , isSampleOf , isResultOfUsedProcedure , isResultOfMadeBySampler . \n  If they are present, the value of these properties summarize the values of the matching properties of the member samples, where membership is either direct or transitive through one or more member sample collections. \n\n  The following consistency rules apply with respect to the Sample properties listed above: \n  \n  1. Where an individual Sample Collection omits a property, a member Sample (direct or transitive) MAY have any value for that property.\n\n  2. Where an individual Sample Collection has a single value for a property, each member Sample (direct or transitive) MUST have that same value for the property - i.e., the collection is homogeneous in that property.  That property MAY be omitted in any member Sample or Sample Collection.\n\n  3. Where an individual Sample Collection has more than one value for a property, each member Sample (direct or transitive) MUST have a value for that property that matches one of the values for the property in the collection. \n  \n  4. Where an individual Sample Collection has a value for a property that is a range or interval, each member Sample (direct or transitive) MUST have a value for that property that either matches or falls within that range or interval.\n\n  5. Where an individual Sample Collection has more than one value for a property that is a range or interval, each member Sample (direct or transitive) MUST have a value for that property that either matches or falls within one of those ranges or intervals.\n  "@en;
        skos:note         "\n  The members of a collection do not necessarily share a common value for any property. \n  " .

sosa:Sample  rdf:type     owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Sample"@en;
        rdfs:subClassOf   prov:Entity;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:SamplingProcedure;
                            owl:onProperty     sosa:isResultOfUsedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sampler;
                            owl:onProperty     sosa:isResultOfMadeBySampler
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:isSampleOfUltimateFOI
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sample;
                            owl:onProperty     sosa:hasOriginalSample
                          ];
        rdfs:subClassOf   [ rdf:type            owl:Restriction;
                            owl:minCardinality  "1"^^xsd:nonNegativeInteger;
                            owl:onProperty      sosa:isSampleOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:isSampleOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Sampling sosa:SamplingCollection )
                                               ];
                            owl:onProperty     sosa:isResultOf
                          ];
        skos:definition   "\n  piece or subset that is intended to be representative of a FeatureOfInterest\n  "@en;
        skos:example      "A statistical sample is often designed to be characteristic of an entire population, so that observations can be made regarding the sample that provide a good estimate of the properties of the population."@en , "A 'station' is a spatial sample, in the form of an identifiable locality where a sensor system or Procedure may be deployed and an observation made. In the context of the observation model, it connotes the 'world in the vicinity of the station', so the observed properties relate to the physical medium at the station, and not to any physical artifact such as a mooring, buoy, benchmark, monument, well, etc."@en;
        skos:note         "\n  Samples are typically subsets or extracts from an entity or feature.\n  Every sample is expected to (eventually) be the feature of interest of an Observation. \n  \n  Samples are used in situations where observations cannot be made directly on the ultimate feature of interest, either because the entire feature cannot be observed, or because it is more convenient to use a proxy.\n  Samples are thus artifacts of an observational strategy, and have no significant function outside of their role in the observation process. \n  The characteristics of the samples themselves are of little interest, except perhaps to the manager of a sampling campaign.\n\n  A Sample is intended to sample some FatureOfInterest, so there is an expectation of at least one isSampleOf property. \n  However, in some cases the identity, and even the exact type, of the sampled feature may not be known when observations are made using the sample.\n\n  Physical samples are sometimes known as 'specimens'. A transient sample, such as a ships-track or flight-line, might be identified and described, but is unlikely to be revisited exactly.\n  "@en .

sosa:MaterialSample  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-sam:;
        rdfs:label        "Material Sample"@en;
        rdfs:subClassOf   sosa:Sample;
        skos:definition   "\n  subset of a FeatureOfInterest that is physical (i.e., tangible)\n  "@en;
        skos:example      "A piece of rock, a blood sample, a water sample, etc."@en;
        skos:note         "MaterialSamples may be destroyed in connexion with the observation act or a subsequent preparation step."@en , "MaterialSamples that are curated and preserved are sometimes known as 'specimens'."@en .

ssn-sam:  rdf:type           owl:Ontology;
        dcterms:created      "2024-02-02"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];
        dcterms:description  "Sampling classes and properties for the SSN Ontology."@en;
        dcterms:license      <http://www.opengeospatial.org/ogc/Software> , <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document>;
        dcterms:rights       "Copyright 2025 W3C/OGC.";
        dcterms:title        "Sensor, Observation, Sample, and Actuator (SOSA) Ontology - actuation module"@en;
        owl:imports          ssn-common: , sosa-sam: .

sosa:wasObservedBy  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "was observed by"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:ObservationCollection , sosa:Observation;
        owl:inverseOf          sosa:observedProperty;
        skos:definition        "\n  relation from a Property to an Observation that observed it\n  "@en .

sosa:wasOriginatedBy  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "was originated by"@en;
        schema:domainIncludes  sosa:ObservationCollection , sosa:Observation;
        schema:rangeIncludes   sosa:Stimulus;
        owl:inverseOf          sosa:originated;
        skos:definition        "\n  relation from an Observation to the Stimulus that originated it\n  "@en .

sosa:resultQuality  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "observation result quality";
        schema:domainIncludes  sosa:ObservationCollection , sosa:Observation;
        owl:inverseOf          sosa:qualityOf;
        skos:definition        "\n  relation from an Observation or ObservationCollection to some information pertaining to the quality of the result\n  "@en;
        skos:note              "dqv:hasQualityMeasurement from the Data Quality Vocabulary is equivalent" , "\n  The quality of a result can be assessed following the procedures in the ISO 19157 series.\n  ";
        skos:scopeNote         "\n  This instance-specific description complements the description of the observation Procedure, which provides information concerning the quality of all observations using this procedure. Multiple measures can be provided.\n  "@en .

sosa:relatedObservation
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "related observation"@en;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ExecutionCollection , sosa:Execution;
        schema:rangeIncludes   sosa:ObservationCollection , sosa:Observation;
        owl:inverseOf          sosa:observationRelatedTo;
        skos:definition        "\n  relation from an Execution or ExecutionCollection to an Observation or ObservationCollection\n  "@en;
        skos:scopeNote         "This general relationship complements the specific predicates inbound to Observation. It is particularly useful to relate Observations (and collections) to other Executions (and collections)" .

sosa:qualityOf  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy      sosa-obs:;
        rdfs:label            "quality of";
        schema:rangeIncludes  sosa:ObservationCollection , sosa:Observation;
        owl:inverseOf         sosa:resultQuality;
        skos:definition       "\n  link from a quality description to an Observation or ObservationCollection that it relates to\n  "@en .

sosa:originated  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "originated"@en;
        schema:domainIncludes  sosa:Stimulus;
        schema:rangeIncludes   sosa:Observation;
        owl:inverseOf          sosa:wasOriginatedBy;
        skos:definition        "\n  relation from a Stimulus to an Observation that was originated by it\n  "@en .

sosa:observes  rdf:type        owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "observes"@en;
        rdfs:subPropertyOf     sosa:forProperty;
        schema:domainIncludes  sosa:Sensor;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:isObservedBy;
        skos:definition        "\n  relation from a Sensor to a Property that it is capable of sensing\n  "@en .

sosa:observedProperty
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "observed property"@en;
        schema:domainIncludes  sosa:ObservationCollection , sosa:Observation;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:wasObservedBy;
        skos:definition        "\n  relation from an Observation or ObservationCollection to the property that was observed\n  "@en;
        skos:note              "\n  'observed property' covers the concepts referred to in [VIM] as 'measurand' (quantitative values) and 'examinand' (categorical and nominal values  [VIM4]), as well as complex and structured properties whose values may be represented as lists, vectors, arrays, rasters, geometries, etc. More specific terms are used in other contexts for related concepts, such as 'analyte' (chemistry) and 'determinand' (water quality).\n  "@en .

sosa:observationRelatedTo
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "observation related to"@en;
        schema:domainIncludes  sosa:ObservationCollection , sosa:Observation;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ExecutionCollection , sosa:Execution;
        owl:inverseOf          sosa:relatedObservation;
        skos:definition        "\n  relation from an Observation or ObservationCollection to another Execution or ExecutionCollection\n  "@en;
        skos:scopeNote         "This general relationship complements the specific predicates outbound from Observation. It is particularly useful to relate Observations (and collections) to other Executions (and collections)" .

sosa:madeObservation  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "made observation"@en;
        rdfs:subPropertyOf     sosa:madeExecution;
        schema:domainIncludes  sosa:Sensor;
        schema:rangeIncludes   sosa:ObservationCollection , sosa:Observation;
        owl:inverseOf          sosa:madeBySensor;
        skos:definition        "\n  relation from a Sensor to an Observation or ObservationCollection it has made\n  "@en .

sosa:madeBySensor  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "made by sensor or observer"@en;
        rdfs:subPropertyOf     sosa:madeBySystem;
        schema:domainIncludes  sosa:ObservationCollection , sosa:Observation;
        schema:rangeIncludes   sosa:Sensor;
        owl:inverseOf          sosa:madeObservation;
        skos:definition        "\n  relation from an Observation or ObservationCollection to the Sensor that made it\n  "@en .

sosa:isProxyFor  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "is proxy for"@en;
        schema:domainIncludes  sosa:Stimulus;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:hasProxy;
        skos:definition        "\n  relation from a Stimulus to the Property for which it is serving as a proxy\n  "@en;
        skos:example           "For example, the expansion of quicksilver is a stimulus that serves as a proxy for some temperature property. An increase or decrease in the velocity of spinning cups on a wind sensor is serving as a proxy for the wind speed."@en .

sosa:isObservedBy  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "is observed by"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:Sensor;
        owl:inverseOf          sosa:observes;
        skos:definition        "\n  relation from a Property to a Sensor that is able to observe it\n  "@en .

sosa:isDetectedBy  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "is detected by"@en;
        schema:domainIncludes  sosa:Stimulus;
        schema:rangeIncludes   sosa:Sensor;
        owl:inverseOf          sosa:detects;
        skos:definition        "\n  relation from a Stimulus to a Sensor that may detect it\n\n  The Stimulus itself will be serving as a proxy for some Property.\n  "@en .

sosa:hasProxy  rdf:type        owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "has proxy"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:Stimulus;
        owl:inverseOf          sosa:isProxyFor;
        skos:definition        "\n  relation from an Property to a Stimulus that serves as its proxy\n  "@en;
        skos:example           "\nFor example, the expansion of mercury is a Stimulus that serves as a proxy for some temperature Property.\nAn increase or decrease in the velocity of spinning cups on a wind Sensor is serving as a proxy for the wind speed.\n  "@en .

sosa:detects  rdf:type         owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-obs:;
        rdfs:label             "detects"@en;
        schema:domainIncludes  sosa:Sensor;
        schema:rangeIncludes   sosa:Stimulus;
        owl:inverseOf          sosa:isDetectedBy;
        skos:definition        "\n  relation from a Sensor to the Stimulus that it detects\n\n  The Stimulus itself will be serving as a proxy for some Property.\n  "@en .

sosa:Stimulus  rdf:type   owl:Class;
        rdfs:isDefinedBy  sosa-obs:;
        rdfs:label        "Stimulus"@en;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:isProxyFor
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sensor;
                            owl:onProperty     sosa:isDetectedBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Observation sosa:ObservationCollection )
                                               ];
                            owl:onProperty     sosa:originated
                          ];
        skos:definition   "\n  event in the real world that triggers a Sensor\n  "@en;
        skos:note         "The properties associated to the Stimulus may be different to the eventual observed Property. It is the event, not the object, that triggers the Sensor."@en .

sosa:Sensor  rdf:type     owl:Class;
        rdfs:isDefinedBy  sosa-obs:;
        rdfs:label        "Sensor or Observer"@en;
        rdfs:subClassOf   sosa:System;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Observation sosa:ObservationCollection )
                                               ];
                            owl:onProperty     sosa:madeObservation
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ObservingProcedure;
                            owl:onProperty     sosa:implements
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:observes
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Stimulus;
                            owl:onProperty     sosa:detects
                          ];
        skos:altLabel     "Observer"@en;
        skos:definition   "\n  System that implements an ObservingProcedure to determine the value of an observable Property\n\n  This may be a device, a piece of infrastructure, an agent (including humans), or software (simulation).\n\n  A Sensor responds to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations.\n  "@en;
        skos:example      "Accelerometers, gyroscopes, barometers, magnetometers, and so forth are Sensors that are typically mounted on a modern smartphone (which acts as Host or Platform). Other examples of Sensors include the human eyes."@en;
        skos:note         "The UML class 'Observer' from ISO 19156:2023 is implemented in SOSA by the OWL/RDFS class 'Sensor'. The class name 'Sensor' is preferred for backward compatibility with existing SOSA deployments. ";
        skos:prefLabel    "Sensor"@en .

sosa:ObservingProcedure
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-obs:;
        rdfs:label        "Observing Procedure"@en;
        rdfs:subClassOf   sosa:Procedure;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sensor;
                            owl:onProperty     sosa:implementedBy
                          ];
        skos:definition   "\n  workflow, protocol, plan, algorithm, or computational method specifying how to make an Observation\n\n  An Observing Procedure is re-usable, and might be involved in many Observations. \n  It explains the steps to be carried out to arrive at reproducible results.\n  "@en;
        skos:example      "A workflow, protocol, plan, algorithm, or computational method specifying how to make an observation; the description of the process used by an observer. This could be a chemical analysis method, a protocol for measuring an object, or even a checklist used by a human observer during a biodiversity campaign. The Procedure could further describe the algorithms behind simulators or models used to generate a result from other inputs."@en;
        skos:note         "[ISO 19156:2023] An ObservingProcedure shall be defined as the description of steps performed in order to determine the value of an observableProperty by an Observer."@en .

sosa:ObservationCollection
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-obs:;
        rdfs:label        "Observation Collection"@en;
        rdfs:subClassOf   sosa:ExecutionCollection;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasUltimateFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sensor;
                            owl:onProperty     sosa:madeBySensor
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:observedProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ObservingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Observation sosa:ObservationCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        owl:disjointWith  sosa:ActuationCollection , sosa:SampleCollection , sosa:SamplingCollection;
        skos:definition   "\n  set of one or more Observations or ObservationCollections; collections may be nested\n\n  All Observation properties may appear in an Observation Collection, including: \n  hasFeatureOfInterest , hasUltimateFeatureOfInterest , usedProcedure , madeBySensor , observedProperty , phenomenonTime , startTime, resultTime , hasResult , hasSimpleResult : \n  If they are present, the value of these properties summarize the values of the matching properties of the member observations, where membership is either direct or transitive through one or more member observation collections. \n  The following consistency rules apply with respect to the Observation properties listed above: \n  \n  1. Where an individual Observation Collection omits a property, a member Observation (direct or transitive) MAY have any value for that property.\n\n  2. Where an individual Observation Collection has a single value for a property, each member Observation(direct or transitive) MUST have that same value for the property - i.e., the collection is homogeneous in that property.  That property MAY then be omitted in any member Observation or Observation Collection.\n\n  3. Where an individual Observation Collection has more than one value for a property, each member Observation (direct or transitive) MUST have a value for that property that matches one of the values for the property in the collection. \n  \n  4. Where an individual Observation Collection has a value for a property that is a range or interval, each member Observation (direct or transitive) MUST have a value for that property that matches or falls within that range or interval.\n\n  5. Where an individual Observation Collection has more than one value for a property that is a range or interval, each member Observation (direct or transitive) MUST have a value for that property that either matches or falls within one of those ranges or intervals.\n  "@en;
        skos:note         "The members of a collection do not necessarily share a common value for any property. "@en , "The results of collections of observations are often packaged in a 'data cube' whose axes define the\n      range of properties of the set of observations." , "An instance of ObservationCollection represents a container for a set of data derived from observations.\n  This broadly corresponds with the class Dataset from DCAT."@en .

sosa:Observation  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-obs:;
        rdfs:label        "Observation"@en;
        rdfs:subClassOf   sosa:Execution;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Stimulus;
                            owl:onProperty     sosa:wasOriginatedBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:observedProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasUltimateFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:ObservingProcedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sensor;
                            owl:onProperty     sosa:madeBySensor
                          ];
        skos:definition   "\n  act of carrying out an ObservingProcedure using a Sensor to estimate or calculate a value of an observable Property of a FeatureOfInterest\n  "@en;
        skos:example      "The activity of estimating the intensity of an Earthquake using the Mercalli intensity scale is an Observation as is measuring the moment magnitude, i.e., the energy released by said earthquake."@en;
        skos:note         "\n  When the feature of interest cannot be observed directly, a sample of it might be used in Observations as a proxy for it. In such case, the Sample is the proximate Feature of Interest of the Observation, while the (ultimate) Feature of Interest of the act of Sampling (i.e., the entity that the Sample is ultimately 'isSampleOf') is the ultimate Feature of Interest of the Observation.\n  ";
        skos:scopeNote    "\n  An Observation primarily concerns a single Property of a single FeatureOfInterest. \n  Observations of multiple properties, or other combinations of Observations, may be described in an ObservationCollection. \n  \n  Alternatively, applications may choose to encapsulate complexity by defining a complex property with multiple individual components, and a corresponding complex result (e.g., a vector). \n  This approach is not prohibited by the SSN Ontology, but the details are beyond the scope of SSN.\n  "@en .

ssn-obs:  rdf:type           owl:Ontology;
        dcterms:created      "2024-02-02"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];
        dcterms:description  "Observation classes and properties for the SSN Ontology."@en;
        dcterms:license      <http://www.opengeospatial.org/ogc/Software> , <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document>;
        dcterms:rights       "Copyright 2025 W3C/OGC.";
        dcterms:title        "Sensor, Observation, Sample, and Actuator (SOSA) Ontology - actuation module"@en;
        owl:imports          ssn-common: , sosa-obs: .

sosa:isMemberOf  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is member of collection";
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling , sosa:SampleCollection , sosa:Sample , sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:ActuationCollection , sosa:Actuation;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:SampleCollection , sosa:ExecutionCollection , sosa:ObservationCollection , sosa:ActuationCollection;
        owl:inverseOf          sosa:hasMember;
        skos:definition        "\n  link from an Execution or Sample or Collection of these, to a collection that it is part of\n  ";
        skos:editorialNote     "Note that there is a single `member` property. OWL or SHACL constraints may limit the range depending on the context of different collection types";
        skos:scopeNote         "rules for the consistency of values of properties of collections and properties of their members are given for each collection type" .

sosa:hasMember  rdf:type       owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has member of collection";
        schema:domainIncludes  sosa:SamplingCollection , sosa:SampleCollection , sosa:ExecutionCollection , sosa:ObservationCollection , sosa:ActuationCollection;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling , sosa:SampleCollection , sosa:Sample , sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:Actuation , sosa:ActuationCollection;
        owl:inverseOf          sosa:isMemberOf;
        skos:definition        "\n  link from a Collection to an Execution or Sample or Collection of these that is part of it\n  ";
        skos:editorialNote     "Note that there is a single `member` property. OWL or SHACL constraints may limit the range depending on the context of different collection types";
        skos:scopeNote         "rules for the consistency of values of properties of collections and properties of their members are given for each collection type" .

sosa:systemDeployment
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "system deployment"@en;
        rdfs:subPropertyOf     sosa:hasDeployment;
        schema:domainIncludes  sosa:System;
        schema:rangeIncludes   sosa:Deployment;
        owl:inverseOf          sosa:deployedSystem;
        skos:definition        "\n  relation from a System to a Deployment\n  "@en .

sosa:madeExecution  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "made execution"@en;
        schema:domainIncludes  sosa:System , sosa:Actuator , sosa:Sensor , sosa:Sampler;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling , sosa:Execution , sosa:ExecutionCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Observation , sosa:ObservationCollection;
        owl:inverseOf          sosa:madeBySystem;
        skos:definition        "\n  relation from a System to an Execution or ExecutionCollection that it has made\n  "@en;
        skos:scopeNote         "\n  madeExecution generalizes the madeActuation, madeObservation, and madeSampling properties. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n\n  madeExecution MUST be considered to be abstract, so SHOULD NOT appear in data instances. \n  Applications can use concrete sub-properties of madeExecution.\n  "@en .

sosa:isSubSystemOf  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is subsystem of"@en;
        schema:domainIncludes  sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        schema:rangeIncludes   sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        owl:inverseOf          sosa:hasSubSystem;
        skos:definition        "\n  relation from a component System to a System that it belongs to\n  "@en .

sosa:isHostedBy  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is hosted by"@en;
        schema:domainIncludes  sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:Platform , sosa:System;
        schema:rangeIncludes   sosa:Platform;
        owl:inverseOf          sosa:hosts;
        skos:definition        "\n  relation from an asset (System or Platform), to a Platform that it is mounted on or hosted by\n  "@en .

sosa:inDeployment  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "in deployment"@en;
        rdfs:subPropertyOf     sosa:hasDeployment;
        schema:domainIncludes  sosa:Platform;
        schema:rangeIncludes   sosa:Deployment;
        owl:inverseOf          sosa:deployedOnPlatform;
        skos:definition        "\n  relation from a Platform to a Deployment\n  "@en;
        skos:example           "For example, a relation between a buoy and a deployment of several Sensors."@en .

sosa:implements  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "implements"@en;
        schema:domainIncludes  sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        schema:rangeIncludes   sosa:SamplingProcedure , sosa:ActuatingProcedure , sosa:ObservingProcedure , sosa:Procedure;
        owl:inverseOf          sosa:implementedBy;
        skos:definition        "\n  relation from a System to the Procedure that it is capable of executing\n  "@en;
        skos:example           "For example, the relationship between a sensor and the scientific measuring Procedure via which it senses."@en .

sosa:hosts  rdf:type            owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-common:;
        rdfs:label              "hosts"@en;
        schema:domainIncludes   sosa:Platform;
        schema:rangeIncludes    sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System , sosa:Platform;
        owl:inverseOf           sosa:isHostedBy;
        owl:propertyChainAxiom  ( sosa:inDeployment sosa:deployedSystem );
        skos:definition         "\n  relation from a Platform to an asset (System or Platform) hosted or mounted on it\n  "@en .

sosa:hasSubSystem  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has subsystem"@en;
        schema:domainIncludes  sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        schema:rangeIncludes   sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        owl:inverseOf          sosa:isSubSystemOf;
        skos:definition        "\n  relation from a System to its component parts\n  "@en .

sosa:hasDeployment  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has deployment"@en;
        schema:domainIncludes  sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:Platform , sosa:System;
        schema:rangeIncludes   sosa:Deployment;
        owl:inverseOf          sosa:deployedAsset;
        skos:definition        "\n  relation from an asset (System or Platform) to a Deployment\n  "@en .

sosa:deployedSystem  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "deployed system"@en;
        rdfs:subPropertyOf     sosa:deployedAsset;
        schema:domainIncludes  sosa:Deployment;
        schema:rangeIncludes   sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        owl:inverseOf          sosa:systemDeployment;
        owl:inverseOf          [ rdfs:subPropertyOf
                          sosa:hasDeployment ];
        skos:definition        "\n  relation from a Deployment to a deployed System\n  "@en .

sosa:deployedOnPlatform
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "deployed on platform"@en;
        rdfs:subPropertyOf     sosa:deployedAsset;
        schema:domainIncludes  sosa:Deployment;
        schema:rangeIncludes   sosa:Platform;
        owl:inverseOf          sosa:inDeployment;
        skos:definition        "\n  relation from a Deployment to a Platform on which Systems are hosted\n  "@en .

sosa:deployedAsset  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "deployed asset"@en;
        schema:domainIncludes  sosa:Deployment;
        schema:rangeIncludes   sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System , sosa:Platform;
        owl:inverseOf          sosa:hasDeployment;
        skos:definition        "\n  relation from a Deployment to an asset (System or Platform)\n  "@en .

sosa:System  rdf:type     owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "System"@en;
        rdfs:subClassOf   prov:Entity , prov:Agent;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Platform;
                            owl:onProperty     sosa:isHostedBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Procedure;
                            owl:onProperty     sosa:implements
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:forProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:hasSubSystem
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:isSubSystemOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Deployment;
                            owl:onProperty     sosa:hasDeployment
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Execution sosa:ExecutionCollection )
                                               ];
                            owl:onProperty     sosa:madeExecution
                          ];
        skos:definition   "\n  entity that implements a Procedure in the context of an Execution\n  "@en;
        skos:note         "\n  This may be a device, a piece of infrastructure, an agent (including humans), or software (simulation).\n\n  Different Systems can implement the same (reusable) Procedure for different Executions.\n\n  A System may have components, its sub-systems, which are other systems.\n  "@en;
        skos:scopeNote    "\n  System generalizes the Actuator, Sensor, and Sampler classes. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n\n  System MUST be considered to be abstract, so any individual System SHOULD be declared to be an instance of a concrete sub-class, and not of the System class.\n  "@en , "\n  Specific system types may be implemented as sub-classes of this class or its sub-classes.\n  Their descriptions may capture the properties and characteristics of a whole class of systems, corresponding with the information typically found in a data-sheet.\n\n  Individual system instances are members of this class or its sub-classes. \n  "@en , "\n  The association of a System (Actuator, Sensor, and Sampler) with a FeatureOfInterest is usually in the context of an Execution or a Deployment. \n\n  A System can be hosted by a Platform. \n\n  A System can be deployed for a specified time interval as part of a Deployment.\n  "@en .

sosa:Platform  rdf:type   owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Platform or Host"@en;
        rdfs:subClassOf   prov:Entity;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Deployment;
                            owl:onProperty     sosa:inDeployment
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:System sosa:Platform )
                                               ];
                            owl:onProperty     sosa:hosts
                          ];
        dcterms:source    <https://inspire.ec.europa.eu/id/document/tg/ef>;
        skos:altLabel     "Host";
        skos:definition   "\n  entity that hosts other entities, particularly Systems and other Platforms\n  "@en;
        skos:example      "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators.\n  An environmental monitoring facility. \n  A platform that hosts a set of sensors. \n  A team performing a survey, where each team member would be modeled as an Observer."@en;
        skos:prefLabel    "Platform";
        skos:scopeNote    "The INSPIRE 'Environmental Monitoring Facility' may be implemented using SOSA by the OWL/RDFS class 'Platform'" .

sosa:Deployment  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Deployment"@en;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:deployedSystem
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Platform;
                            owl:onProperty     sosa:deployedOnPlatform
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ rdf:type     owl:Class;
                                                 owl:unionOf  ( sosa:Platform sosa:System )
                                               ];
                            owl:onProperty     sosa:deployedAsset
                          ];
        skos:definition   "\n  arrangement of one or more assets (Systems or Platforms) to execute Procedures with respect to designated features of interest.\n  "@en;
        skos:example      "A temperature Sensor deployed on a wall, a network of Sensors deployed for an Observation campaign."@en;
        skos:note         "\n  A Deployment will usually be for a specified time interval. \n  It MAY involve Platforms, or Systems, or both. \n  A Deployment can be used quite flexibly to describe the use of systems, and platforms that host systems, with respect to a feature of interest. \n  "@en .

sosa:usedProcedure  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy        sosa-common:;
        rdfs:label              "used procedure"@en;
        schema:domainIncludes   sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Sampling , sosa:SamplingCollection;
        schema:rangeIncludes    sosa:SamplingProcedure , sosa:Procedure , sosa:ObservingProcedure , sosa:ActuatingProcedure;
        owl:inverseOf           sosa:usedForExecution;
        owl:propertyChainAxiom  ( sp:executionAssociation sp:hadProcedure );
        skos:definition         "\n  relation from an Execution or ExecutionCollection to a Procedure used to make it\n  "@en .

sosa:startTime  rdf:type       owl:DatatypeProperty;
        rdfs:comment           "The value would usually be encoded using xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear"@en;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "start time"@en;
        rdfs:subPropertyOf     prov:startedAtTime;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution , sosa:Deployment;
        skos:definition        "\n  instant of time when the Execution or Deployment was initiated or tasked\n  "@en .

sosa:resultTime  rdf:type      owl:DatatypeProperty;
        rdfs:comment           "The value would usually be encoded using xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear"@en;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "result time"@en;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        skos:definition        "\n  instant of time when the result of the Execution became available\n  "@en .

sosa:phenomenonTime  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "phenomenon time"@en;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        schema:rangeIncludes   time:TemporalEntity;
        owl:inverseOf          sosa:phenomenonOccurred;
        skos:definition        "\n  time that the result of an Execution applies to the subject entity\n\n  It may be an interval or an instant, or some other compound temporal entity [[!owl-time]].\n  "@en;
        skos:scopeNote         "\n  The phenomenonTime is not necessarily the same as the resultTime. \n  \n  In the case of Actuation, phenomenonTime SHOULD indicate the time during which the actuator was active. \n\n  In the case of Observation, the phenomenonTime MAY be distant from the resultTime. \n  For example in the case of forecasts the phenomenonTime is after the resultTime. \n  In historical, geological and cosmological investigations the phenomenonTime can be far in the past, while the resultTime is contemporary. \n\n  In the case of Sampling the phenomenonTime SHOULD indicate the time during which the Sampling Procedure was active.\n  The resultTime can indicate when the sample comes into the posession of the operator. \n  "@en .

sosa:phenomenonOccurred
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "phenomenon occurred"@en;
        schema:domainIncludes  time:TemporalEntity;
        schema:rangeIncludes   sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        owl:inverseOf          sosa:phenomenonTime;
        skos:definition        "\n  result of an Execution that this time applies to\n  "@en .

sosa:madeBySystem  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "made by system"@en;
        rdfs:subPropertyOf     prov:wasAssociatedWith;
        schema:domainIncludes  sosa:Execution , sosa:ExecutionCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Observation , sosa:ObservationCollection , sosa:Sampling , sosa:SamplingCollection;
        schema:rangeIncludes   sosa:Sampler , sosa:Sensor , sosa:System , sosa:Actuator;
        owl:inverseOf          sosa:madeExecution;
        skos:definition        "\n  relation from an Execution to the System that made it\n  "@en;
        skos:scopeNote         "\n  madeBySystem generalizes the madeByActuator, madeBySensor, and madeBySampler properties. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n\n  madeBySystem MUST be considered to be abstract, so SHOULD NOT appear in data instances. \n  Applications can use concrete sub-properties of madeBySystem.\n  "@en .

sosa:isResultOf  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is result of"@en;
        rdfs:subPropertyOf     prov:wasGeneratedBy;
        schema:domainIncludes  sosa:Sample;
        schema:rangeIncludes   sosa:Sampling , sosa:Actuation , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        owl:inverseOf          sosa:hasResult;
        skos:definition        "\n  relation from a result to the Execution that created or caused it\n  "@en .

sosa:inputValueForExecution
        rdf:type              owl:ObjectProperty;
        rdfs:isDefinedBy      sosa-common:;
        rdfs:label            "input value for execution"@en;
        schema:rangeIncludes  sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        owl:inverseOf         sosa:hasInputValue;
        skos:definition       "\n  relation from an input value to an Execution that used it\n  "@en .

sosa:hasUltimateFeatureOfInterest
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has ultimate feature of interest"@en;
        schema:domainIncludes  sosa:Deployment , sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Sampling , sosa:SamplingCollection;
        schema:rangeIncludes   sosa:FeatureOfInterest;
        skos:definition        "\n  relation from a Deployment or Execution to its ultimate subject entity\n\n  i.e., from an Actuation to the ultimate entity whose property was modified;\n  from an Observation to the ultimate entity whose property was observed;\n  from an act of Sampling to the ultimate entity that was sampled;\n  or from a Deployment to its ultimate target\n  "@en;
        skos:note              "should match a property chain something like this but with sosa:isSampleOf*\n      owl:propertyChainAxiom ( sosa:hasFeatureOfInterest sosa:isSampleOf )";
        skos:scopeNote         "This is useful when the proximate feature of interest is a sample of the ultimate feature of interest, directly or transitively." .

sosa:hasSimpleResult  rdf:type  owl:FunctionalProperty , owl:DatatypeProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has simple result"@en;
        schema:domainIncludes  sosa:ActuationCollection , sosa:Actuation , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        skos:definition        "\n  relation from an Execution to its value expressed as a literal\n\n  In the case of an Actuation, the result may be the value of the Property following the execution of the Procedure by the Actuator. \n\n  In the case of an Observation, the result is the value of the Property produced by the execution of the Procedure by the Sensor. \n  "@en;
        skos:example           "For instance, the values 23 or true."@en;
        skos:note              "This DatatypeProperty provides an alternative to hasResult, to be used only if the execution result can be expressed as a Literal"@en .

sosa:hasResult  rdf:type       owl:ObjectProperty , owl:FunctionalProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has result"@en;
        rdfs:subPropertyOf     prov:generated;
        schema:domainIncludes  sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Sampling , sosa:SamplingCollection;
        schema:rangeIncludes   sosa:Sample;
        owl:inverseOf          sosa:isResultOf;
        skos:definition        "\n  relation from an Execution to its result\n\n  In the case of an Actuation, the result may be the value of the Property following the execution of the Procedure by the Actuator. \n\n  In the case of an Observation, the result is the value of the Property produced by the execution of the Procedure by the Sensor. \n  \n  In the case of a Sampling, the result is the Sample produced by the execution of the Procedure by the Sampler. \n\n  An Execution has a single result, therefore sosa:hasResult is Functional.\n  "@en;
        skos:note              "The result value MUST be consistent with the hasOutput definition from the corresponding Procedure"@en .

sosa:hasInputValue  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has input value"@en;
        schema:domainIncludes  sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:Observation , sosa:ExecutionCollection , sosa:Execution;
        skos:definition        "\n  assigns a value to an input defined by the Procedure that is used in an Execution\n  "@en;
        skos:note              "The input value MUST be consistent with a hasInput definition from the corresponding Procedure"@en;
        skos:scopeNote         "The input might be a calibration or conditioning parameter, or the output of a previous procedure within an extended processing chain."@en .

sosa:hasFeatureOfInterest
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has feature of interest"@en;
        rdfs:subPropertyOf     prov:used;
        schema:domainIncludes  sosa:SamplingCollection , sosa:Deployment , sosa:Execution , sosa:ExecutionCollection , sosa:Observation , sosa:ObservationCollection , sosa:Actuation , sosa:ActuationCollection , sosa:Sampling;
        schema:rangeIncludes   sosa:FeatureOfInterest;
        owl:inverseOf          sosa:isFeatureOfInterestOf;
        skos:definition        "\n  relation from an Execution or Deployment to its subject entity\n\n  i.e., from an Actuation to the entity whose property was modified;\n  from an Observation to the entity whose quality was observed;\n  from an act of Sampling to the entity that was sampled;\n  or from a Deployment to its target\n  "@en;
        skos:example           "For example, in an Observation of the weight of a person, the FeatureOfInterest is the person and the property is its weight."@en;
        skos:scopeNote         "This is the _proximate_ feature of interest, which may be a proxy entity such as a sample of the _ultimate_ feature of interest. Use `hasUltimateFeatureOfInterest` if the intention is to link to the _ultimate_ feature of interest. " .

sosa:endTime  rdf:type         owl:DatatypeProperty;
        rdfs:comment           "The value would usually be encoded using xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear"@en;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "end time"@en;
        rdfs:subPropertyOf     prov:endedAtTime;
        schema:domainIncludes  sosa:ActuationCollection , sosa:Actuation , sosa:Deployment , sosa:ExecutionCollection , sosa:Execution;
        skos:definition        "\n  instant of time when the Execution or Deployment was ended or completed\n  "@en .

sosa:Result  rdf:type     owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Result"@en;
        rdfs:subClassOf   [ rdf:type            owl:Restriction;
                            owl:onProperty      sosa:isResultOf;
                            owl:someValuesFrom  sosa:Execution
                          ];
        owl:deprecated    true;
        skos:definition   "\n  outcome of an Observation, Actuation, or act of Sampling. \n  \n  To store an observation's simple result value one can use the hasSimpleResult property.\n  "@en;
        skos:example      "The value 20 as the height of a certain tree together with the unit, e.g., Meter."@en .

sosa:ExecutionCollection
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Execution Collection"@en;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Execution sosa:ExecutionCollection )
                                               ];
                            owl:onProperty     sosa:hasMember
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasUltimateFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Procedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:madeBySystem
                          ];
        skos:definition   "\n  set of one or more Executions or ExecutionCollections; collections may be nested\n\n  All Execution properties may appear in an Execution Collection, including: \n  hasFeatureOfInterest , usedProcedure , madeBySystem , endTime, resultTime , startTime . \n  If they are present, the value of these properties summarize the values of the matching properties of the member Executions, where membership is either direct or transitive through one or more member execution collections.\n\n  The following consistency rules apply with respect to the execution properties listed above: \n  \n  1. Where an individual Execution Collection omits a property, a member Execution (direct or transitive) MAY have any value for that property.\n\n  2. Where an individual Execution Collection has a single value for a property, each member Execution (direct or transitive) MUST have that same value for the property - i.e., the collection is homogeneous in that property.  That property MAY then be omitted in any member Execution or Execution Collection.\n\n  3. Where an individual Execution Collection has more than one value for a property, each member Execution (direct or transitive) MUST have a value for that property that matches one of the values for the property in the collection. \n  \n  4. Where an individual Execution Collection has a value for a property that is a range or interval, each member Execution (direct or transitive) MUST have a value for that property that matches or falls within that range or interval.\n\n  5. Where an individual Execution Collection has more than one value for a property that is a range or interval, each member Execution (direct or transitive) MUST have a value for that property that either matches or falls within one of those ranges or intervals.\n  "@en;
        skos:note         "The members of a collection do not necessarily share a common value for any property. "@en;
        skos:scopeNote    "\n  Execution Collection generalizes the Actuation Collection, Observation Collection, and Sampling Collection classes. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n\n  Execution Collection MUST be considered to be abstract, so any individual Execution Collection SHOULD be declared to be an instance of one of the concrete sub-classes, and not of the Execution Collection class.\n  "@en .

sosa:Execution  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Execution of a Procedure"@en;
        rdfs:subClassOf   prov:Activity;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasUltimateFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:hasFeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type            owl:Restriction;
                            owl:onProperty      sosa:hasFeatureOfInterest;
                            owl:someValuesFrom  sosa:FeatureOfInterest
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Procedure;
                            owl:onProperty     sosa:usedProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:madeBySystem
                          ];
        skos:definition   "\n  act of carrying out a Procedure using a System\n  "@en;
        skos:scopeNote    "\n  Execution generalizes the Actuation, Observation, and Sampling classes. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n\n  Execution MUST be considered to be abstract, so any individual Execution SHOULD be declared to be an instance of one of the concrete sub-classes, and not of the Execution class.\n  "@en .

sosa:usedForExecution
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "used for execution"@en;
        schema:domainIncludes  sosa:Procedure;
        schema:rangeIncludes   sosa:Sampling , sosa:Actuation , sosa:Observation , sosa:Execution;
        owl:inverseOf          sosa:usedProcedure;
        skos:definition        "\n  relation from a Procedure to an Execution or ExecutionCollection made using it\n  "@en .

sosa:outputFor  rdf:type      owl:ObjectProperty;
        rdfs:isDefinedBy      sosa-common:;
        rdfs:label            "output for"@en;
        schema:rangeIncludes  sosa:Procedure;
        owl:inverseOf         sosa:hasOutput;
        skos:definition       "\n  relation from an output to a Procedure that generates it in an Execution\n  "@en .

sosa:isProcedureFor  rdf:type  owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is procedure for"@en;
        rdfs:subPropertyOf     sosa:forProperty;
        schema:domainIncludes  sosa:Procedure;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:hasProcedure;
        skos:definition        "\n  relation from a Procedure to a Property that it can observe or act on\n  "@en .

sosa:inputFor  rdf:type       owl:ObjectProperty;
        rdfs:isDefinedBy      sosa-common:;
        rdfs:label            "input for"@en;
        schema:rangeIncludes  sosa:Procedure;
        owl:inverseOf         sosa:hasInput;
        skos:definition       "\n  relation from an input to a Procedure that uses it in an Execution\n  "@en .

sosa:implementedBy  rdf:type   owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "implemented by"@en;
        schema:domainIncludes  sosa:Procedure;
        schema:rangeIncludes   sosa:Sampler , sosa:Actuator , sosa:Sensor , sosa:System;
        owl:inverseOf          sosa:implements;
        skos:definition        "\n  relation from a Procedure to a System that is capable of executing it\n  "@en;
        skos:example           "For example, the relationship between a scientific measuring Procedure and a sensor that senses via that Procedure."@en .

sosa:hasProcedure  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has procedure"@en;
        rdfs:subPropertyOf     sosa:propertyFor;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:Procedure;
        owl:inverseOf          sosa:isProcedureFor;
        owl:inverseOf          [ rdfs:subPropertyOf
                          sosa:forProperty ];
        skos:definition        "\n  relation from a Property to a Procedure that can observe or act on it\n  "@en .

sosa:hasOutput  rdf:type       owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has output"@en;
        schema:domainIncludes  sosa:Procedure;
        owl:inverseOf          sosa:outputFor;
        skos:definition        "\n  relation from a Procedure to an output of its execution\n  "@en;
        skos:note              "This property will typically describe the type of the result from executing the Procedure."@en .

sosa:hasInput  rdf:type        owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has input"@en;
        schema:domainIncludes  sosa:Procedure;
        owl:inverseOf          sosa:inputFor;
        skos:definition        "\n  relation from a Procedure to an input required for its execution\n  "@en;
        skos:note              "This property will typically describe the type of the inputs required to execute the Procedure."@en;
        skos:scopeNote         "An input might be a calibration or conditioning parameter, or the output of a previous procedure within an extended processing chain."@en .

sosa:Procedure  rdf:type  owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Procedure"@en;
        rdfs:subClassOf   prov:Plan;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:forProperty
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:System;
                            owl:onProperty     sosa:implementedBy
                          ];
        skos:definition   "\n  workflow, protocol, plan, algorithm, or computational method specifying how to make an Execution\n\n  A Procedure is re-usable, and might be applied in many Actuations, Observations, or Samplings.\n  It explains the steps to be carried out to arrive at reproducible results.\n  "@en;
        skos:example      "The measured wind speed differs depending on the height of the sensor above the surface, e.g., due to friction. Consequently, procedures for measuring wind speed define a standard height for anemometers above ground, typically 10m for meteorological measures and 2m in Agrometeorology. This definition of height, sensor placement, and so forth are defined by the Procedure."@en;
        skos:scopeNote    "\n  Procedure generalizes the ActuationProcedure, ObservationProcedure, and SamplingProcedure classes. \n  It is included in the ontology to support the definition of a common pattern for executions and their properties. \n  "@en .

sosa:propertyFor  rdf:type     owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "property for"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:Actuator , sosa:Sensor , sosa:System , sosa:Procedure;
        owl:inverseOf          sosa:forProperty;
        skos:definition        "\n  relation from a Property to some Procedure or System that acts on it or observes it\n  "@en;
        skos:example           "For example, from a Sensor to the properties it can observe; from an Actuator to the properties it can act on; from a SystemCapability to the Property the capability is described for."@en .

sosa:isUltimateFeatureOfInterestOf
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is ultimate feature of interest of"@en;
        schema:domainIncludes  sosa:FeatureOfInterest;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution , sosa:Deployment;
        owl:inverseOf          sosa:hasUltimateFeatureOfInterest;
        skos:definition        "\n  relation from an ultimate FeatureOfInterest to an Execution, Execution Collection, or Deployment, for which it is the ultimate subject\n  "@en .

sosa:isPropertyOf  rdf:type    owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is property of"@en;
        schema:domainIncludes  sosa:Property;
        schema:rangeIncludes   sosa:FeatureOfInterest;
        owl:inverseOf          sosa:hasProperty;
        skos:definition        "\n  relation from a Property to a FeatureOfInterest that it is a quality of\n  "@en .

sosa:isFeatureOfInterestOf
        rdf:type               owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "is feature of interest of"@en;
        schema:domainIncludes  sosa:FeatureOfInterest;
        schema:rangeIncludes   sosa:SamplingCollection , sosa:Sampling , sosa:ActuationCollection , sosa:Actuation , sosa:ObservationCollection , sosa:Observation , sosa:ExecutionCollection , sosa:Execution , sosa:Deployment;
        owl:inverseOf          sosa:hasFeatureOfInterest;
        skos:definition        "\n  relation from a FeatureOfInterest to an Execution, Execution Collection, or Deployment, operating on it\n\n  i.e., to an Observation about it, to an Actuation acting on it, to an act of Sampling that sampled it,\n  or to a Deployment for which it is the subject.\n"@en .

sosa:hasProperty  rdf:type     owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "has property"@en;
        schema:domainIncludes  sosa:FeatureOfInterest;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:isPropertyOf;
        skos:definition        "\n  relation from an entity to a Property of that entity\n  "@en .

sosa:forProperty  rdf:type     owl:ObjectProperty;
        rdfs:isDefinedBy       sosa-common:;
        rdfs:label             "for property"@en;
        schema:domainIncludes  sosa:Actuator , sosa:Sensor , sosa:System , sosa:Procedure;
        schema:rangeIncludes   sosa:Property;
        owl:inverseOf          sosa:propertyFor;
        skos:definition        "\n  relation from some Procedure or System to a Property it acts on or observes\n  "@en;
        skos:example           "For example, from a Sensor to the properties it can observe; from an Actuator to the properties it can act on; from a SystemCapability to the Property the capability is described for."@en .

sosa:Property  rdf:type   owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Property"@en;
        rdfs:subClassOf   prov:Entity;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Procedure;
                            owl:onProperty     sosa:hasProcedure
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:FeatureOfInterest;
                            owl:onProperty     sosa:isPropertyOf
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Actuation sosa:ActuationCollection )
                                               ];
                            owl:onProperty     sosa:wasActedOnBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Actuator;
                            owl:onProperty     sosa:isActedOnBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Stimulus;
                            owl:onProperty     [ owl:inverseOf  sosa:isProxyFor ]
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  [ owl:unionOf  ( sosa:Observation sosa:ObservationCollection )
                                               ];
                            owl:onProperty     sosa:wasObservedBy
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sensor;
                            owl:onProperty     sosa:isObservedBy
                          ];
        skos:definition   "\n  identifiable quality of features of interest that can be observed or acted upon\n\n  A property can apply to different features of interest.\n\n  "@en;
        skos:example      "Cars (a feature type) all have a characteristic colour, where 'colour' is a property."@en;
        skos:note         "Concepts from existing code lists, vocabularies, and taxonomies, may be used as instances of Property"@en .

sosa:FeatureOfInterest
        rdf:type          owl:Class;
        rdfs:isDefinedBy  sosa-common:;
        rdfs:label        "Feature Of Interest"@en;
        rdfs:subClassOf   prov:Entity;
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Property;
                            owl:onProperty     sosa:hasProperty
                          ];
        rdfs:subClassOf   [ rdf:type            owl:Restriction;
                            owl:onProperty      sosa:isFeatureOfInterestOf;
                            owl:someValuesFrom  [ rdf:type     owl:Class;
                                                  owl:unionOf  ( sosa:Deployment sosa:Execution sosa:ExecutionCollection )
                                                ]
                          ];
        rdfs:subClassOf   [ rdf:type           owl:Restriction;
                            owl:allValuesFrom  sosa:Sample;
                            owl:onProperty     sosa:hasSample
                          ];
        skos:definition   "\n  entity that is the target of an Execution or Deployment\n\n  i.e., whose property is being estimated by an Observation, \n  or whose property is being manipulated by an Actuator, \n  or which is being sampled or transformed in an act of Sampling, \n  or which is the target of Executions planned as part of a Deployment.\n  "@en;
        skos:example      "When measuring the height of a tree, the height is the observed Property, 20m may be the Result of the Observation, and the tree is the FeatureOfInterest. A window is a FeatureOfInterest for an automatic window control Actuation."@en .

sosa-common:  rdf:type       owl:Ontology;
        dcterms:created      "2024-02-02"^^xsd:date;
        dcterms:creator      [ rdf:type   foaf:Agent;
                               foaf:name  "W3C/OGC Spatial Data on the Web Working Group"@en
                             ];

[ rdf:type                     utility-services:Connection;
  utility-services:identifier  [ rdf:type       adms:Identifier;
                                 skos:notation  "SUBB001"^^<https://example.com/concept/identificatortype/intern_aansluitingsnummer>
                               ];
  utility-services:inNetwork   _:b0
] .

_:b0    rdf:type                        utility-services:UtilityNetwork;
        dc:type                         nutsvoorzieningnetwerktype:elektriciteit;
        utility-services:authorityRole  [ rdf:type        org:Organization;
                                          skos:prefLabel  "Rijksarchief te Brussel"@nl
                                        ] .

[ rdf:type                     utility-services:Connection;
  utility-services:identifier  [ rdf:type                     adms:Identifier;
                                 utility-services:identifier  "SUBA001"^^<https://example.com/concept/identificatortype/intern_aansluitingsnummer>
                               ];
  utility-services:inNetwork   _:b0
] .

[ rdf:type                     utility-services:Connection;
  network:beginLink            [ rdf:type         utility-services:UtilityLink;
                                 network:endNode  "_:AS003"
                               ];
  network:beginLink            [ rdf:type         utility-services:UtilityLink;
                                 network:endNode  "_:AS002"
                               ];
  utility-services:identifier  [ rdf:type       adms:Identifier;
                                 skos:notation  "541234567890123456"^^<https://example.com/concept/identificatortype/ean_nummer>
                               ];
  utility-services:inNetwork   [ rdf:type                        utility-services:UtilityNetwork;
                                 dc:type                         nutsvoorzieningnetwerktype:elektriciteit;
                                 utility-services:authorityRole  [ rdf:type        org:Organization;
                                                                   skos:prefLabel  "Fluvius"@nl
                                                                 ]
                               ]
] .

unit:EUR  rdf:type  qudt:Unit .

[ rdf:type                   sosa:Observation;
  dct:type                   observatietype:meting;
  sosa:hasFeatureOfInterest  _:b0;
  sosa:hasResult             [ rdf:type         schema:QuantitativeValue;

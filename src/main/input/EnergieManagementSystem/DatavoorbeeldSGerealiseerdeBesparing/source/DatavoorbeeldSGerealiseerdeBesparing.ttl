                               schema:value     "2.74E1"^^rdfs:Literal
                             ];
  sosa:observedProperty      observeerbaar_kenmerk:gemiddeld_dagelijks_verbruik;
  sosa:phenomenonTime        [ rdf:type           time:ProperInterval;
                               time:hasBeginning  [ rdf:type            time:Instant;
                                                    time:inXSDDateTime  "20240101T00:00:00.000"^^xs:dateTime
                                                  ];
                               time:hasEnd        [ rdf:type            time:Instant;
                                                    time:inXSDDateTime  "20241231T23:59:59.000"^^xs:dateTime
                                                  ]
                             ];
  sosa:usedProcedure         [ rdf:type  <http://www.w3.org/ns/sosa/oms/ObservingProcedure>;
                               dct:type  observatieproceduretype:berekening
                             ]
] .

[ rdf:type                   sosa:Observation;
